{"ast":null,"code":"import '@/assets/css/chartMain.css';\nimport axios from 'axios';\nexport default {\n  name: \"ChartMainView\",\n  components: {},\n  data() {\n    return {\n      // 초기값으로 현재 날짜를 설정\n      selectedDate: new Date().toISOString().substring(0, 10),\n      cartList: [],\n      totalCount: \"\"\n    };\n  },\n  methods: {\n    numberWithCommas(x) {\n      return x.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\n    },\n    dateChange() {\n      console.log('date:', this.selectedDate);\n      console.log(\"데이터 가져오기\");\n      console.log(this.selectedDate);\n      axios({\n        method: 'get',\n        //put,post,delete\n        url: 'http://localhost:9000/attention/manager/chart/main',\n        headers: {\n          \"Content-Type\": \"application/json; charset=utf-8\"\n        },\n        //전송타입\n        params: {\n          selectedDate: this.selectedDate\n        },\n        //get방식 파라미터로 값이 전달\n        //data: guestbookVo, //put, post, delete 방식 자동으로 JSON으로 변환 전달\n\n        responseType: 'json' //수신타입\n      }).then(response => {\n        console.log(response.data); //수신데이타\n        this.cartList = response.data.apiData;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  computed: {\n    totalPricesSum() {\n      return this.cartList.reduce((acc, item) => acc + item.totalPrice, 0);\n    }\n  },\n  created() {\n    this.dateChange();\n  }\n};","map":{"version":3,"names":["axios","name","components","data","selectedDate","Date","toISOString","substring","cartList","totalCount","methods","numberWithCommas","x","toString","replace","dateChange","console","log","method","url","headers","params","responseType","then","response","apiData","catch","error","computed","totalPricesSum","reduce","acc","item","totalPrice","created"],"sources":["C:\\javastudy\\kiosk\\vue-pos\\src\\views\\chart\\ChartMainView.vue"],"sourcesContent":["<template>\r\n    <div class=\"wrap\">\r\n        <header class=\"chartMainHeader\">\r\n            <router-link to=\"/setting\" class=\"home\"><img src=\"@/assets/images/home_icon.png\">\r\n            </router-link>\r\n            <router-link to=\"/setting\" class=\"logoImg\"><img src=\"@/assets/images/attention.png\"></router-link>\r\n            <ul>\r\n\r\n                <li><router-link to=\"/manager\">메뉴등록</router-link></li>\r\n                <li><router-link to=\"/chart/main\">총매출</router-link></li>\r\n                <li><router-link to=\"/chart/coffeeview\">카테고리매출</router-link></li>\r\n\r\n            </ul>\r\n        </header>\r\n        <!-- <h1>매출현황</h1> -->\r\n\r\n        <div>\r\n            <div class=\"chartMainGorup\">\r\n                <div class=\"titleGroup\">\r\n                    <label>\r\n                        <router-link to=\"/chart/main\"> 매출현황</router-link>\r\n                    </label>\r\n\r\n                </div>\r\n                <div class=\"chartMainGorup2\">\r\n                    <table>\r\n                        <!-- <tr>\r\n                                <td rowspan=\"8\">총 매출액</td>\r\n                            </tr> -->\r\n                        <tr>\r\n                            <td colspan=\"2\">총매출</td>\r\n                            <!-- <th>매출액</th> -->\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">{{ numberWithCommas(totalPricesSum) }}</td>\r\n\r\n                            <td><input type=\"date\" v-model=\"selectedDate\" v-on:change=\"dateChange\"></td>\r\n\r\n                        </tr>\r\n                        <tr>\r\n                            <td>카테고리명</td>\r\n                            <td>수량</td>\r\n                            <td>금액</td>\r\n                        </tr>\r\n\r\n                        <tr v-bind:key=\"i\" v-for=\"(AttentionCartVo, i) in cartList\">\r\n                            <td>{{ AttentionCartVo.category }}</td>\r\n                            <td> {{ AttentionCartVo.totalCount }}</td>\r\n                            <td> {{ numberWithCommas(AttentionCartVo.totalPrice) }}</td>\r\n                        </tr>\r\n\r\n\r\n                    </table>\r\n\r\n                </div>\r\n                \r\n            </div> <!-- categoryGroup2 -->\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport '@/assets/css/chartMain.css';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: \"ChartMainView\",\r\n    components: {\r\n    },\r\n    data() {\r\n        return {\r\n            // 초기값으로 현재 날짜를 설정\r\n            selectedDate: new Date().toISOString().substring(0, 10),\r\n            cartList: [],\r\n            totalCount: \"\"\r\n\r\n        };\r\n    },\r\n    methods: {\r\n        numberWithCommas(x) {\r\n            return x.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, ',');\r\n        },\r\n        dateChange() {\r\n            console.log('date:', this.selectedDate);\r\n            console.log(\"데이터 가져오기\");\r\n            console.log(this.selectedDate);\r\n\r\n            axios({\r\n                method: 'get',  //put,post,delete\r\n                url: 'http://localhost:9000/attention/manager/chart/main',\r\n                headers: { \"Content-Type\": \"application/json; charset=utf-8\" }, //전송타입\r\n                params: { selectedDate: this.selectedDate }, //get방식 파라미터로 값이 전달\r\n                //data: guestbookVo, //put, post, delete 방식 자동으로 JSON으로 변환 전달\r\n\r\n                responseType: 'json' //수신타입\r\n            }).then(response => {\r\n                console.log(response.data); //수신데이타\r\n                this.cartList = response.data.apiData;\r\n\r\n\r\n            }).catch(error => {\r\n                console.log(error);\r\n            });\r\n        },\r\n\r\n\r\n    },\r\n    computed: {\r\n        totalPricesSum() {\r\n            return this.cartList.reduce((acc, item) => acc + item.totalPrice, 0);\r\n        }\r\n    },\r\n    created() {\r\n        this.dateChange();\r\n    }\r\n    \r\n};\r\n</script>\r\n\r\n\r\n<style></style>\r\n"],"mappings":"AAgEA,OAAO,4BAA4B;AACnC,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE,CACZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACH;MACAC,YAAY,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,SAAS,CAAC,CAAC,EAAE,EAAE,CAAC;MACvDC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IAEhB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,gBAAgBA,CAACC,CAAC,EAAE;MAChB,OAAOA,CAAC,CAACC,QAAQ,CAAC,CAAC,CAACC,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC7D,CAAC;IACDC,UAAUA,CAAA,EAAG;MACTC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAE,IAAI,CAACb,YAAY,CAAC;MACvCY,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;MACvBD,OAAO,CAACC,GAAG,CAAC,IAAI,CAACb,YAAY,CAAC;MAE9BJ,KAAK,CAAC;QACFkB,MAAM,EAAE,KAAK;QAAG;QAChBC,GAAG,EAAE,oDAAoD;QACzDC,OAAO,EAAE;UAAE,cAAc,EAAE;QAAkC,CAAC;QAAE;QAChEC,MAAM,EAAE;UAAEjB,YAAY,EAAE,IAAI,CAACA;QAAa,CAAC;QAAE;QAC7C;;QAEAkB,YAAY,EAAE,MAAK,CAAE;MACzB,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAChBR,OAAO,CAACC,GAAG,CAACO,QAAQ,CAACrB,IAAI,CAAC,EAAE;QAC5B,IAAI,CAACK,QAAO,GAAIgB,QAAQ,CAACrB,IAAI,CAACsB,OAAO;MAGzC,CAAC,CAAC,CAACC,KAAK,CAACC,KAAI,IAAK;QACdX,OAAO,CAACC,GAAG,CAACU,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EAGJ,CAAC;EACDC,QAAQ,EAAE;IACNC,cAAcA,CAAA,EAAG;MACb,OAAO,IAAI,CAACrB,QAAQ,CAACsB,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,UAAU,EAAE,CAAC,CAAC;IACxE;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACnB,UAAU,CAAC,CAAC;EACrB;AAEJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}